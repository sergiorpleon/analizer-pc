{
    "name": "sergiorpleon/analizer-pc",
    "description": "Sistema de bÃºsqueda de componentes PC usando embeddings vectoriales con Ollama y PostgreSQL",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergio RP Leon",
            "email": "sergiorpleon@example.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*",
        "ext-pgsql": "*",
        "dompdf/dompdf": "^3.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --testdox",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": "dist",
        "platform-check": false
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
